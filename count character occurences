class Main {
    public static void main(String[] args) {
        String s = "Program";
        int[] c = new int[256];
        for (char ch : s.toCharArray()) {
            c[ch]++;
        }
        System.out.println("Character Occurrences:");
        for (int i = 0; i < c.length; i++) {
            if (c[i] > 0) {
                System.out.println((char) i + " : " + c[i]);
            }
        }
    }
}
