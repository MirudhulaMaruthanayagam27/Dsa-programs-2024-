class Main {
    int countFreq(int[] arr, int target) {
        int count = 0; 
        for (int num : arr) { 
            if (num == target) { 
                count++; 
            }
        }
        return count; 
    }
    public static void main(String[] args) {
        Main sol = new Main(); 
        int[] arr = {1, 2, 3, 2, 3, 4}; 
        int target = 2;
        int count = sol.countFreq(arr, target); 
        System.out.println(+count); 
    }
}
