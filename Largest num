public class Main{
    public static void main(String args[]){
        int[] arr={12,34,6,78,5};
        int n=arr.length;
        int large=arr[0];12/
        for(int i=0;i<n;i++){
            if(arr[i]>large){
                large=arr[i];
            }
        }
         System.out.println("The largest number is: " + large);
    }
}
